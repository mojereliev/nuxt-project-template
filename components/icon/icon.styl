.icon
  display block
  width 1em
  height 1em
  vertical-align top
  color currentColor

  use
    width 100%
    height 1em
    fill currentColor

  &_behance
    width (21em / 14)

  &_branch-left
  &_branch-right
    width em(40 / 70 * $baseFontSize)

  &_facebook
    width (7em / 15)

  &_logo
    width em(188 / 30 * $baseFontSize)

  &_payse
    width (11em / 17)

  &_play
    width (14em / 15)

  &_share
    width (13em / 16)
